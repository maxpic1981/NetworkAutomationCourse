{% for node,inv in nodes|dictsort %}
{%   if inventory_hostname == node %}
router bgp {{inv.as}}
{%     for neighbor,def in inv.ebgp|dictsort %}
{% 	     if def.ipv4 is defined %}
 neighbor {{def.ipv4}} remote-as {{def.neighbor_as}}
{%       endif %}
{%     endfor %}
address-family ipv4
{%     for neighbor,def in inv.ebgp|dictsort %}
{% 	     if def.ipv4 is defined %}
 neighbor {{def.ipv4}} activate
{%       endif %}
{%     endfor %}
{%   endif %}
{% endfor %}
end
